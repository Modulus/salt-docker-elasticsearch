## Denne er for elastic search master



cluster.name: "kosekroken"

http:
    cors:
        enabled: true
        allow-origin: "/.*/"

node:
  name: {{ name }}
  data: false
  master: true
  ingest: false

network:
    bind_host: _site_
    publish_host: {{ip}}

discovery:
  zen:
    minimum_master_nodes: 1
    ping:
      unicast:
        hosts: {{ servers }}
    ping_timeout: 3s
    fd:
      ping_timeout: 3s
      ping_retries: 10

## Disable X-Pack
#xpack.security.enabled: false
#xpack.monitoring.enabled: false
#xpack.ml.enabled: false
#xpack.watcher.enabled: false